{
  "name": "sparrowhawk",
  "version": "0.0.1",
  "description": "An Angular2 native mobile app to help you practice Korean, flashcard-style, on the go",
  "keywords": [
    "korean",
    "hangeul",
    "hangul",
    "language",
    "flashcards",
    "flash-cards",
    "memorization",
    "memorize"
  ],
  "author": "Nara Kasbergen <nara.kasbergen@gmail.com>",
  "license": "GPL-3.0",
  "homepage": "https://github.com/xiehan/sparrowhawk#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xiehan/sparrowhawk.git"
  },
  "bugs": {
    "url": "https://github.com/xiehan/sparrowhawk/issues"
  },
  "main": "src/app/index.ts",
  "scripts": {
    "postinstall": "typings install",
    "update": "npm prune && npm cache clean && npm install",
    "init": "gulp init",
    "lint": "tslint 'src/**/*.ts'",
    "start": "npm run start:ios",
    "start:ios": "gulp start.ios",
    "start:android": "gulp start.android",
    "test": "gulp test.browser/ci",
    "tdd": "gulp test.browser",
    "pree2e": "npm run webdriver:update -- --standalone",
    "e2e": "protractor",
    "e2e:ios": "npm run e2e -- test/protractor.conf.ios.dev.js",
    "e2e:ios:ci": "npm run e2e -- test/protractor.conf.ios.ci.js",
    "e2e:android": "npm run e2e -- test/protractor.conf.android.dev.js",
    "e2e:android:ci": "npm run e2e -- test/protractor.conf.android.ci.js",
    "webdriver:update": "webdriver-manager update",
    "webdriver:start": "webdriver-manager start"
  },
  "dependencies": {
    "angular2-react-native": "0.0.1-alpha.15",
    "firebase": "^3.3.0",
    "react": "~15.2.1",
    "react-native-material-kit": "^0.3.2"
  },
  "devDependencies": {
    "codelyzer": "0.0.28",
    "del": "^1.2.0",
    "gulp": "^3.9.0",
    "gulp-env": "^0.4.0",
    "gulp-filter": "^3.0.1",
    "gulp-preprocess": "^2.0.0",
    "gulp-strip-comments": "^2.2.1",
    "gulp-typescript": "^2.11.0",
    "gulp-watch": "~4.3.5",
    "jasmine": "2.4.1",
    "karma": "^0.13.21",
    "karma-chrome-launcher": "^0.2.2",
    "karma-jasmine": "^0.3.6",
    "karma-nyan-reporter": "^0.2.4",
    "protractor": "^3.3.0",
    "run-sequence": "~1.1.2",
    "systemjs": "^0.19.25",
    "through2": "~0.6.3",
    "tslint": "^3.14.0",
    "tslint-eslint-rules": "^1.3.0",
    "typescript": "^1.8.10",
    "wd": "^0.4.0",
    "wd-bridge": "^0.0.2"
  },
  "engines": {
    "node": ">= 4.1",
    "npm": ">= 2.14.3"
  },
  "private": true
}
